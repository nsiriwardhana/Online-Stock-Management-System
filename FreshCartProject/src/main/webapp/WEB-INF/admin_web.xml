<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
		http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
		version="3.1">
		
		
	<servlet>
		<servlet-name>admin_login</servlet-name>
		<servlet-class>com.admin.admin_loginServelet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>admin_login</servlet-name>
		<url-pattern>/admin_loginServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>insertProduct</servlet-name>
		<servlet-class>com.admin.insertProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>insertProduct</servlet-name>
		<url-pattern>/insertProductServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addSupplier</servlet-name>
		<servlet-class>com.admin.addSupplierServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>addSupplier</servlet-name>
		<url-pattern>/addSupplierServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateSupplier</servlet-name>
		<servlet-class>com.admin.UpdateSupplierServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateSupplier</servlet-name>
		<url-pattern>/UpdateSupplierServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UpdateProduct</servlet-name>
		<servlet-class>com.admin.UpdateProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UpdateProduct</servlet-name>
		<url-pattern>/UpdateProductServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteProduct</servlet-name>
		<servlet-class>com.admin.DeleteProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteProduct</servlet-name>
		<url-pattern>/DeleteProductServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteSupplier</servlet-name>
		<servlet-class>com.admin.DeleteSupplierServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteSupplier</servlet-name>
		<url-pattern>/DeleteSupplierServlet</url-pattern>
	</servlet-mapping>
	
	
</web-app>